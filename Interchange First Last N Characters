import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ch = new Scanner(System.in);
        String s = ch.nextLine();
        int n = ch.nextInt();
        int len = s.length();
        String a = s.substring(0, n);
        String b = s.substring(n, len - n);
        String c = s.substring(len - n, len);
        String result = c + b + a;
        System.out.println(result);
    }
}


Sample Input 
mango
2
Sample Output
gonma
